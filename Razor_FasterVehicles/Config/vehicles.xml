	<!-- Gyrocopter			9, 9, 15, 9		-->
	<!-- Jokeblimp			18, 18, 25, 25	-->
	<!-- Bicycle			6, 4, 8.5, 4	-->
	<!-- Minibike			7, 4, 9.2, 4	-->
	<!-- Motorcycle			9.8, 6, 14, 8	-->
	<!-- Truck				10, 8, 14, 10	-->
