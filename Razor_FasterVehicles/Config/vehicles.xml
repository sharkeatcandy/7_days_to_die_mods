<configs>
	<!-- Makes changes to all vehicles's velocityMax_turbo to our desired value -->
	<!-- The original values of each vehicles are as followed: -->
	<!-- Gyrocopter			9, 9, 15, 9		-->
	<!-- Jokeblimp			18, 18, 25, 25	-->
	<!-- Bicycle			6, 4, 8.5, 4	-->
	<!-- Minibike			7, 4, 9.2, 4	-->
	<!-- Motorcycle			9.8, 6, 14, 8	-->
	<!-- Truck				10, 8, 14, 10	-->
	<!-- The latter value is maximum speed a vehicle can reach; in meter per second. -->
	<set xpath="/vehicles/vehicle[@name='vehicleGyrocopter']/property[@name='velocityMax_turbo']/@value">30, 20, 40, 20</set>
	<set xpath="/vehicles/vehicle[@name='vehicleJokeblimp']/property[@name='velocityMax_turbo']/@value">40, 40, 50, 50</set>
	<set xpath="/vehicles/vehicle[@name='vehicleBicycle']/property[@name='velocityMax_turbo']/@value">30, 20, 40, 20</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMinibike']/property[@name='velocityMax_turbo']/@value">30, 20, 40, 20</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax_turbo']/@value">30, 20, 40, 20</set>
	<set xpath="/vehicles/vehicle[@name='vehicleTruck4x4']/property[@name='velocityMax_turbo']/@value">30, 20, 40, 20</set>
